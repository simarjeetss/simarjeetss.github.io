{
  "headers": [
    {
      "source": "/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "trailingSlash": false,
  "images": {
    "sizes": [640, 750, 828, 1080, 1200],
    "minimumCacheTTL": 60
  }
}
